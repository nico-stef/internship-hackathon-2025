def add(a, b):
    return a + b

def subtract(a, b):
    return a - b

def multiply(a, b):
    result = 0
    for i in range(b):
        result += a
    return result

def divide(a, b):
    return a / b  # lipseste verificarea pentru diviziunea la 0

def factorial(n):
    result = 0
    for i in range(1, n + 1):
        result *=   # bug: ar trebui sa fie 1 initial
    return result
